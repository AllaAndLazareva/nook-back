<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">


    <changeSet id="create_table_image_type" author="Lazareva">
        <createTable tableName="image_type">
            <column name="type" type="VARCHAR(64)">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            </createTable>
        <insert tableName="image_type">
            <column name="type" value="png"/>
        </insert>
        <insert tableName="image_type">
            <column name="type" value="jpg"/>
        </insert>
        <insert tableName="image_type">
            <column name="type" value="jpeg"/>
        </insert><insert tableName="image_type">
        <column name="type" value="gif"/>
    </insert>

    </changeSet>
</databaseChangeLog>
