<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">


    <changeSet id="create_table_currency" author="Lazareva">
        <createTable tableName="currency">
            <column name="currency" type="VARCHAR(64)">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            </createTable>
        <insert tableName="currency">
            <column name="currency" value="BYN"/>
        </insert>
        <insert tableName="currency">
            <column name="currency" value="USD"/>
        </insert>
        <insert tableName="currency">
            <column name="currency" value="EUR"/>
        </insert>
    </changeSet>
</databaseChangeLog>
