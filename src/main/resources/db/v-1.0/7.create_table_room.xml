<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">


    <changeSet id="create_table_room" author="Lazareva">
        <createTable tableName="room">
            <column name="room" type="VARCHAR(64)">
                <constraints nullable="false" primaryKey="true"/>
            </column>
            </createTable>
        <insert tableName="room">
            <column name="room" value="ONE"/>
        </insert>
        <insert tableName="room">
            <column name="room" value="TWO"/>
        </insert>
        <insert tableName="room">
            <column name="room" value="THREE"/>
        </insert>
        <insert tableName="room">
            <column name="room" value="FOUR_AND_MORE"/>
        </insert>
        <insert tableName="room">
            <column name="room" value="ROOM"/>
        </insert>
    </changeSet>
</databaseChangeLog>
